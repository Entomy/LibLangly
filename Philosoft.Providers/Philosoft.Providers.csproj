<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netstandard1.3;netstandard2.1</TargetFrameworks>
		<LangVersion>9.0</LangVersion>
		<Nullable>enable</Nullable>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
		<Description>Provides a rich set of functions that provides implementations of trait methods for you.</Description>
		<PackageTags>trait, traits, trait-based</PackageTags>
		<AssemblyName>Philosoft.Providers</AssemblyName>
		<RootNamespace>System.Traits</RootNamespace>
		<PackageIcon>Logo.png</PackageIcon>
		<CodeAnalysisRuleSet>GlobalRuleSet.ruleset</CodeAnalysisRuleSet>
		<CodeAnalysisRuleSetDirectories>..\</CodeAnalysisRuleSetDirectories>
		<WarningLevel>5</WarningLevel>
	</PropertyGroup>

	<ItemGroup>
		<None Include="Logo.png" Pack="true" Visible="true" PackagePath="\" />
	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\Philosoft.Concepts\Philosoft.Concepts.csproj" />
	</ItemGroup>

</Project>

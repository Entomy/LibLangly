<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netstandard1.3;net6.0</TargetFrameworks>
		<LangVersion>preview</LangVersion>
		<Nullable>enable</Nullable>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
		<Description>Provides proper support for set theory and set algebra.</Description>
		<PackageTags>set, sets, set theory, set algebra</PackageTags>
		<AssemblyName>Numbersome.Sets</AssemblyName>
		<PackageIcon>Logo.png</PackageIcon>
		<CodeAnalysisRuleSet>..\Global.ruleset</CodeAnalysisRuleSet>
	</PropertyGroup>

	<ItemGroup>
		<None Include="Logo.png" Pack="true" Visible="true" PackagePath="\" />
	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\Philosoft.Traits\Philosoft.Traits.csproj" />
	</ItemGroup>

	<ItemGroup Condition="'$(TargetFramework)' == 'net6.0'">
	  <PackageReference Include="System.Runtime.Experimental">
	    <Version>6.0.0-preview.7.21377.19</Version>
	  </PackageReference>
	</ItemGroup>

</Project>

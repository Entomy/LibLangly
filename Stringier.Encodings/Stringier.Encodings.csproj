	<ItemGroup>
	  <PackageReference Include="System.Memory" Version="4.5.4" />
	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\Philosoft.NullableAttributes\Philosoft.NullableAttributes.csproj" />
	  <ProjectReference Include="..\Stringier.Rune\Stringier.Rune.csproj" />
	</ItemGroup>

</Project>

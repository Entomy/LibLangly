<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net5.0</TargetFramework>
		<LangVersion>9.0</LangVersion>
		<Nullable>enable</Nullable>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
		<Description>Provides support parsing pattern expressions.</Description>
		<PackageTags>parse, parser, syntax, syntax tree, abstract syntax tree, parse tree, pattern, patterns</PackageTags>
		<AssemblyName>Langly.Patterns</AssemblyName>
		<RootNamespace>Langly.Patterns</RootNamespace>
		<PackageIcon>Logo.png</PackageIcon>
	</PropertyGroup>

	<ItemGroup>
		<Compile Include="..\GlobalSuppressions.cs" Link="GlobalSuppressions.cs" />
		<None Include="Logo.png" Pack="true" Visible="true" PackagePath="\" />
	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\Langly\Langly.csproj" />
	</ItemGroup>

</Project>

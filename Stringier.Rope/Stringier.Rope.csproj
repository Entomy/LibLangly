	  <ProjectReference Include="..\Stringier.Case\Stringier.Case.csproj" />
	  <ProjectReference Include="..\Stringier.Rune\Stringier.Rune.csproj" />
	</ItemGroup>

	<Import Project="..\Collection\Collection.projitems" Label="Shared" />


<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net5.0</TargetFramework>
		<Description>F# bindings for Philosoft.Extensions.</Description>
		<PackageTags>f#, fsharp</PackageTags>
		<AssemblyName>Philosoft.Extensions.FSharp</AssemblyName>
		<PackageIcon>Logo.png</PackageIcon>
	</PropertyGroup>

	<ItemGroup>
		<None Include="Logo.png" Pack="true" Visible="true" PackagePath="\" />
	</ItemGroup>

	<ItemGroup>
		<Compile Include="Bindings.fs" />
		<Compile Include="Functions.fs" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Philosoft.Extensions\Philosoft.Extensions.csproj" />
	</ItemGroup>

</Project>

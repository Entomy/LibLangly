<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard1.3</TargetFramework>
		<LangVersion>9.0</LangVersion>
		<Nullable>enable</Nullable>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
		<Description>Provides attributes for annotating behavioral characteristics of code, such as asymptotic complexity, and more.</Description>
		<PackageTags>complex, complexity, asymptotic, time, asymptotic time, attribute, attributes, analysis, static analysis</PackageTags>
		<AssemblyName>Philosoft.BehavioralAttributes</AssemblyName>
		<RootNamespace>System.Diagnostics.CodeAnalysis</RootNamespace>
		<PackageIcon>Logo.png</PackageIcon>
		<CodeAnalysisRuleSet>..\Global.ruleset</CodeAnalysisRuleSet>
	</PropertyGroup>

	<ItemGroup>
		<None Include="Logo.png" Pack="true" Visible="true" PackagePath="\" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Philosoft.CodeAnalysisAttributes\Philosoft.CodeAnalysisAttributes.csproj" />
	</ItemGroup>

</Project>
